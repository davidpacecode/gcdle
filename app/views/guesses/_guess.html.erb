<div id="<%= dom_id guess %>" class="w-full sm:w-auto my-5 space-y-5">
  <div>
    <strong class="block font-medium mb-1">Game:</strong>
    <%= guess.game_id %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Value:</strong>
    <%= guess.value %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Guess number:</strong>
    <%= guess.guess_number %>
  </div>
</div>
